#include <iostream>
#include <ITGapi.h>
using namespace std;

int main() {

	char command[10000] = "";

	char host[] = "10.1.1.48";

	char command[] = " -a 10.1.1.95 -rp 9500 -C 100 -c 500 -t 20000";

	DITGsend(host, command);







	return 0;
}
