#include "cfunctions.h"

void string2charvet(const string s, char* vetc) {
	strncpy(vetc, s.c_str(), sizeof(char) * CHAR_BUFFER);
	vetc[sizeof(char) * CHAR_BUFFER - 1] = '\0';
}

