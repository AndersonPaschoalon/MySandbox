#include <iostream>
using namespace std;

int main() {

		bool tcp = true;
		std::string valval = (tcp == true)? std::string("") + std::string("a") + std::string("b") : "udp";
		std::cout << "Hello: " << valval << std::endl;

		char *aaa[2];
		aaa[0] = "00000";
		aaa[1] = "11111";

		std::cout << "aaa[0]:" << aaa[0] << "\naaa[1]:" << aaa[1];


		return 0;
}
